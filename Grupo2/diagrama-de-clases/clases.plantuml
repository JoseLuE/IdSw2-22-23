@startuml modelo-de-dominio
class Compra {
    + descripcion
    + fechaDeCompra

    + Compra()
    + realizarCompra()
}

class Maquina {
    + nombre
    + Dinero[]
    + Productos[]

    + Maquina()
    + totalDinero()
    + totalProductos()

}

class Billete {
    + valor
    + moneda

    + Billete()
}

class Moneda {
    + valor
    + moneda

    + Moneda()
}

class Producto {
    + nombre
    + precio
    + proveedor

    + Producto()
}

class Dinero {
    + valor
    + moneda

    + Dinero()
}

class Mantenimiento {
    + nombre
    + tipo

    + Mantenimiento()
    + generarReposicion()
}

Maquina -u-o Compra
Maquina *--l Mantenimiento
Maquina o-- Dinero
Maquina o---r Producto

Dinero *--- Billete
Dinero *--- Moneda
Mantenimiento ...r.o Dinero

@enduml